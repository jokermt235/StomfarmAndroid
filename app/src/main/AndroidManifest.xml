<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="devel.exesoft.com.accshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:noHistory="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ClientsActivity" />
        <activity android:name=".DebtsActivity" />
        <activity android:name=".PartnerActivity" />
        <activity android:name=".NewPartnerActivity" />
        <activity android:name=".OrdersActivity" />
        <activity android:name=".NewOrderActivtiy" />
        <activity android:name=".NewOrderAddItemManuallyActivity"></activity>
    </application>

</manifest>